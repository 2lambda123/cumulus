{
  "collection": {
    "dynamic_templates": [
      {
        "all_date_fields": {
          "match": "*At",
          "match_mapping_type": "long",
          "mapping": {
            "type": "date"
          }
        }
      }
    ],
    "properties": {
      "name": {
        "type": "keyword"
      },
      "version": {
        "type": "keyword"
      },
      "timestamp": {
        "type": "date"
      }
    }
  }
}
