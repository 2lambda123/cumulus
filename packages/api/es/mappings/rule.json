{
  "rule": {
    "dynamic_templates": [
      {
        "all_date_fileds": {
          "match": "*At",
          "match_mapping_type": "long",
          "mapping": {
            "type": "date"
          }
        }
      }
    ],
    "properties": {
      "name": {
        "type": "keyword"
      },
      "workflow": {
        "type": "keyword"
      },
      "provider": {
        "type": "keyword"
      },
      "collection": {
        "properties": {
          "name": {
            "type": "keyword"
          },
          "version": {
            "type": "keyword"
          }
        }
      },
      "meta": {
        "properties": {
          "type": {
            "type": "string"
          },
          "value": {
            "type": "string"
          }
        }
      },
      "state": {
        "type": "keyword"
      },
      "timestamp": {
        "type": "date"
      }
    }
  }
}