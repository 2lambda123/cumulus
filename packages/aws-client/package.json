{
  "name": "@cumulus/aws-client",
  "version": "10.1.1",
  "description": "Utilities for working with AWS",
  "keywords": [
    "GIBS",
    "CUMULUS",
    "NASA"
  ],
  "engines": {
    "node": ">=12.18.0"
  },
  "files": [
    "*.js",
    "*.d.ts",
    "lib/*.js",
    "lib/*.d.ts"
  ],
  "publishConfig": {
    "access": "public"
  },
  "homepage": "https://github.com/nasa/cumulus/tree/master/packages/aws-client/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/nasa/cumulus"
  },
  "scripts": {
    "build-docs": "npm run tsc && ../../node_modules/.bin/jsdoc2md -t templates/README.hbs *.js > README.md",
    "clean": "git clean -d -x -e node_modules -f",
    "test": "../../node_modules/.bin/ava",
    "test:coverage": "../../node_modules/.bin/nyc npm test",
    "prepare": "npm run tsc",
    "tsc": "../../node_modules/.bin/tsc",
    "tsc:listEmittedFiles": "../../node_modules/.bin/tsc --listEmittedFiles",
    "watch-test": "../../node_modules/.bin/tsc-watch --onsuccess 'npm test'"
  },
  "ava": {
    "files": [
      "tests/**/*.js"
    ],
    "verbose": true
  },
  "author": "Cumulus Authors",
  "license": "Apache-2.0",
  "dependencies": {
    "aws-sdk": "^2.585.0",
    "@aws-sdk/client-api-gateway": "^3.53.0",
    "@aws-sdk/client-cloudformation": "^3.53.0",
    "@aws-sdk/client-cloudwatch-events": "^3.53.0",
    "@aws-sdk/client-cloudwatch-logs": "^3.53.0",
    "@aws-sdk/client-cloudwatch": "^3.53.0",
    "@aws-sdk/client-dynamodb-streams": "^3.53.0",
    "@aws-sdk/client-dynamodb": "^3.53.0",
    "@aws-sdk/client-ec2": "^3.53.0",
    "@aws-sdk/client-ecs": "^3.53.0",
    "@aws-sdk/client-elasticsearch-service": "^3.53.0",
    "@aws-sdk/client-kinesis": "^3.53.0",
    "@aws-sdk/client-kms": "^3.53.0",
    "@aws-sdk/client-lambda": "^3.53.0",
    "@aws-sdk/client-s3": "^3.53.0",
    "@aws-sdk/client-secrets-manager": "^3.53.0",
    "@aws-sdk/client-sfn": "^3.53.0",
    "@aws-sdk/client-sns": "^3.53.0",
    "@aws-sdk/client-sqs": "^3.53.0",
    "@aws-sdk/client-ssm": "^3.53.0",
    "@aws-sdk/client-sts": "^3.53.0",
    "@aws-sdk/lib-dynamodb": "^3.53.0",
    "@aws-sdk/types": "^3.53.0",
    "@cumulus/checksum": "10.1.1",
    "@cumulus/errors": "10.1.1",
    "@cumulus/logger": "10.1.1",
    "jsonpath-plus": "^1.1.0",
    "lodash": "~4.17.21",
    "p-map": "^1.2.0",
    "p-retry": "^4.2.0",
    "p-timeout": "^4.1.0",
    "p-wait-for": "^3.2.0",
    "pump": "^3.0.0"
  }
}
