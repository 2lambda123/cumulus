{
  "name": "@cumulus/cognito-client",
  "version": "8.0.0",
  "description": "A generic auth client",
  "homepage": "https://github.com/nasa/cumulus/tree/master/packages/cognito-client#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nasa/cumulus.git",
    "directory": "packages/cognito-client"
  },
  "author": "Cumulus Authors",
  "license": "Apache-2.0",
  "keywords": [
    "Cumulus"
  ],
  "engines": {
    "node": ">=12.18.0"
  },
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/src/**/*.js",
    "dist/src/**/*.d.ts"
  ],
  "scripts": {
    "clean": "rm -rf dist",
    "build-docs": "npm run tsc && ../../node_modules/.bin/jsdoc2md -t templates/README.md.hbs dist/src/*.js > README.md",
    "test": "../../node_modules/.bin/ava",
    "test:coverage": "../../node_modules/.bin/nyc npm test",
    "prepare": "npm run tsc",
    "tsc": "rm -rf dist && ../../node_modules/.bin/tsc",
    "tsc:listEmittedFiles": "../../node_modules/.bin/tsc --listEmittedFiles"
  },
  "ava": {
    "files": [
      "tests/*"
    ],
    "verbose": true,
    "typescript": {
      "rewritePaths": {
        "tests/": "dist/tests/"
      }
    }
  },
  "dependencies": {
    "@cumulus/auth-client": "8.0.0",
    "got": "^11.7.0"
  },
  "bugs": {
    "url": "https://github.com/nasa/cumulus/issues"
  },
  "directories": {
    "test": "tests"
  },
  "devDependencies": {}
}
